<h1>Booked rooms</h1>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
</head>
<body>
    
  <div id="rooms">  
{{#each data}}
    
<article class="room">
        <img src="/uploads/{{this.roomimage}}" alt="" id="roomImage"> 
        <p id="roomTitle">{{this.title}}</p>
        <p id="roomDescription">{{this.description}} </p>
        <p id="roomPrice">Price:<strong>{{this.price}}</strong> per night</p></strong>
        <p id="roomLocation">Location:{{this.location}}</p>
        <p>From:{{this.from}}</p>
        <p>To:{{this.to}}</p>
        <a href="/room-listing/delete/{{this.id}}?method=DELETE" class="delete">Delete Room</a>
    
</article>
{{else}}
<h1>No Booked Rooms</h1>

{{/each}}
</div>

    
</body>
</html>
